{
  "name": "product-api-server",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=16.17.0",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "test": "mocha",
    "fix": "gts fix",
    "lint": "gts lint && markdownlint-cli2",
    "clean": "gts clean",
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "start": "node -r source-map-support/register dist/server.js",
    "start:all": "npm-run-all -p start:debug start:queue",
    "start:dev": "ts-node-dev --exit-child -r tsconfig-paths/register src/server.ts | pino-pretty",
    "start:queue": "ts-node-dev --exit-child --transpile-only --inspect=0.0.0.0:9002 -r tsconfig-paths/register src/queue.ts | pino-pretty",
    "start:debug": "ts-node-dev --exit-child --transpile-only --inspect=0.0.0.0:9229 -r tsconfig-paths/register src/server.ts | pino-pretty",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev",
    "generate:prisma": "prisma generate"
  },
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^10.0.1",
    "@prisma/client": "^4.13.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.4.0",
    "bull": "^4.10.1",
    "csv-parse": "^5.3.2",
    "dotenv": "^16.0.1",
    "fastify": "^4.10.2",
    "fastify-cors": "^6.1.0",
    "fastify-file-upload": "^4.0.0",
    "fastify-plugin": "^4.0.0",
    "markdownlint-cli2": "^0.5.1",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "pino": "^8.6.1",
    "prisma": "^4.13.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chai": "^4.3.7",
    "eslint": "8.22.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^4.0.0",
    "mocha": "^10.1.0",
    "pino-pretty": "^9.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^4.2.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "^1.3.8",
    "tsconfig-paths": "^3.10.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-json-schema": "^0.54.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
